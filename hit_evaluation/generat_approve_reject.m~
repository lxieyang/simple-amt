function [reject_or_approve] ...
            = generat_approve_reject(sample_entry, sample_coord, assignment_id, image_url, coord_first, ...
                                     coord_second, worker_id, hit_id)
    for i = 1 : numel(assignment_id)
        for j = 1 : numel(image_url{i})
            count = 0;
            for k = 1 : numel(sample_entry)
                if sample_entry{k} == image_url{i}{j}
                    count = count + 1;
                    [percentage(count), temp(count)] = reject_by_intersection_union(sample_coord{k}, 
                end
            end
        end
    end

end